{
  "name": "fleetflow",
  "version": "1.0.0",
  "description": "FleetFlow - Intelligent Fleet Management System built on Znova Framework",
  "keywords": [
    "fleet-management",
    "logistics",
    "vehicle-tracking",
    "driver-management",
    "analytics",
    "znova-framework"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fleetflow.git"
  },
  "scripts": {
    "dev": "npm --prefix frontend run dev",
    "build": "npm --prefix frontend run build",
    "install:all": "npm install --prefix frontend && pip install -r requirements.txt",
    "test:frontend": "npm --prefix frontend run test",
    "start": "python run.py",
    "setup:fresh": "python setup_fresh.py",
    "setup:demo": "python setup_fresh.py --demo"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.8.0"
  }
}
